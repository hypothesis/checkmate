<h1>Hello {{ request.session.user.name }}</h1>

{% set user = request.session.user %}

{% if user.picture %}
<img alt="{{ user.name }} avatar" src="{{ user.picture }}">
{% endif %}
<br>

<h2>Effective Principals</h2>
<code>{{ request.effective_principals }}</code>

<h2>Session</h2>
<code>{{ request.session }}</code>

<hr>
<a href="{{ request.route_url("logout") }}">Logout</a>